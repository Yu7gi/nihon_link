<% users.each do |user| %>
  <tr>
    <td>
      <%= link_to user_path(user.id) do %>
        <% if user.profile_image.attached? %>
          <%= image_tag url_for(user.profile_image), size: "70x70" %>
        <% else %>
          <%= image_tag 'icon_user_5.png', size: "70x70" %>
        <% end %>
        <%= user.name %>
      <% end %>
    </td>
  </tr>
<% end %>